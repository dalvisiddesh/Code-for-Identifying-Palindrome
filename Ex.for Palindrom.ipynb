    "\n",
    "# Read the input\n",
    "s = input()\n",
    "\n",
    "#check for palindrome here\n",
    "s=s.lower()\n",
    "\n",
    "r=\"\"\n",
    "\n",
    "for i in s:\n",
    "    r=i+r\n",
    "if r==s:\n",
    "    print(1)\n",
    "else:\n",
    "    print(0)"
